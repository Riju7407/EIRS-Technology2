{
  "buildCommand": "cd client && npm run build",
  "outputDirectory": "client/build",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "/api"
  }
}
